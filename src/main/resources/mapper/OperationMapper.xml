<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bluett.mapper.OperationMapper">
    <cache eviction="LRU" flushInterval="60000" size="512"/>

    <resultMap id="BaseResultMap" type="org.bluett.entity.OperationDO">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="caseId" column="case_id" jdbcType="INTEGER"/>
            <result property="operate" column="operate" jdbcType="VARCHAR"/>
            <result property="updateDt" column="update_dt" jdbcType="TIMESTAMP"/>
            <result property="createDt" column="create_dt" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,case_id,operate,
        update_dt,create_dt
    </sql>

    <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from operation
        where  id = #{id,jdbcType=INTEGER} 
    </select>
    <select id="selectPageByCaseIdList" resultType="org.bluett.entity.OperationDO">
        select
        <include refid="Base_Column_List" />
        from operation
        where  case_id in
        <foreach collection="list" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
        limit #{page.offset},#{page.size}
    </select>
    <select id="selectCount" resultType="java.lang.Integer">
        select count(1) from operation
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
        delete from operation
        where  id = #{id,jdbcType=INTEGER} 
    </delete>
    <delete id="deleteBatch">
        delete from operation
        where  id in
        <foreach collection="list" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </delete>
    <insert id="insert" keyColumn="id" keyProperty="id" parameterType="org.bluett.entity.OperationDO" useGeneratedKeys="true">
        insert into operation
        ( id,case_id,operate
        ,update_dt,create_dt)
        values (#{id,jdbcType=INTEGER},#{caseId,jdbcType=INTEGER},#{operate,jdbcType=VARCHAR}
        ,#{updateDt,jdbcType=TIMESTAMP},#{createDt,jdbcType=TIMESTAMP})
    </insert>
    <insert id="insertSelective" keyColumn="id" keyProperty="id" parameterType="org.bluett.entity.OperationDO" useGeneratedKeys="true">
        insert into operation
        <trim prefix="(" suffix=")" suffixOverrides=",">
                <if test="id != null">id,</if>
                <if test="caseId != null">case_id,</if>
                <if test="operate != null">operate,</if>
                <if test="updateDt != null">update_dt,</if>
                <if test="createDt != null">create_dt,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
                <if test="id != null">#{id,jdbcType=INTEGER},</if>
                <if test="caseId != null">#{caseId,jdbcType=INTEGER},</if>
                <if test="operate != null">#{operate,jdbcType=VARCHAR},</if>
                <if test="updateDt != null">#{updateDt,jdbcType=TIMESTAMP},</if>
                <if test="createDt != null">#{createDt,jdbcType=TIMESTAMP},</if>
        </trim>
    </insert>
    <update id="updateByPrimaryKeySelective" parameterType="org.bluett.entity.OperationDO">
        update operation
        <set>
                <if test="caseId != null">
                    case_id = #{caseId,jdbcType=INTEGER},
                </if>
                <if test="operate != null">
                    operate = #{operate,jdbcType=VARCHAR},
                </if>
                <if test="updateDt != null">
                    update_dt = #{updateDt,jdbcType=TIMESTAMP},
                </if>
                <if test="createDt != null">
                    create_dt = #{createDt,jdbcType=TIMESTAMP},
                </if>
        </set>
        where   id = #{id,jdbcType=INTEGER} 
    </update>
    <update id="updateByPrimaryKey" parameterType="org.bluett.entity.OperationDO">
        update operation
        set 
            case_id =  #{caseId,jdbcType=INTEGER},
            operate =  #{operate,jdbcType=VARCHAR},
            update_dt =  #{updateDt,jdbcType=TIMESTAMP},
            create_dt =  #{createDt,jdbcType=TIMESTAMP}
        where   id = #{id,jdbcType=INTEGER} 
    </update>
</mapper>
